func funktion(x : int): int {
  return x * x;
} end


func zeile(start : int, _end : int, step : int, schwelle : int): int {
  var dummy : int;
  var x : int;

  x := start;
  while x <= _end do
    if funktion(x) >= schwelle then
      dummy := writeChar(42);
    else
      dummy := writeChar(32);
    end;
    x := x + step;
  end;
  return writeChar(10);
} end


func zeichnen(x1 : int, x2 : int, spalten : int, y1 : int, y2 : int, zeilen : int): int {
  var xstep : int;
  var ystep : int;
  var y : int;
  var dummy : int;

  xstep := (x2 - x1) / spalten;
  ystep := (y2 - y1) / zeilen;
  y := y2;
  while (y >= y1) do
    dummy := zeile(x1, x2, xstep, y);
    y := y - ystep;
  end;
  return 0;
} end


func main(): int {
  return zeichnen(0 - 2000, 2000, 64, 0, 4000000, 20);
} end
